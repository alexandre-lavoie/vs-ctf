{
    "name": "vs-ctf",
    "version": "0.0.1",
    "publisher": "alexandre-lavoie",
    "displayName": "VS CTF",
    "description": "An extension for CTFs",
    "license": "MIT",
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/alexandre-lavoie/vs-ctf"
    },
    "engines": {
        "vscode": "^1.90.0"
    },
    "main": "out/extension.js",
    "scripts": {
        "build": "rollup --config rollup.config.ts --bundleConfigAsCjs --configPlugin @rollup/plugin-typescript",
        "watch": "rollup --watch --config rollup.config.ts --bundleConfigAsCjs --configPlugin @rollup/plugin-typescript"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^12.1.0",
        "@types/node": "^22.7.4",
        "@types/vscode": "^1.90.0",
        "rollup": "^4.24.0",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2"
    },
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vs-ctf",
                    "title": "VS CTF",
                    "icon": "resources/icons/flag.svg"
                }
            ]
        },
        "views": {
            "vs-ctf": [
                {
                    "id": "vs-ctf.challenges",
                    "name": "Challenges"
                },
                {
                    "id": "vs-ctf.scoreboard",
                    "name": "Scoreboard"
                }
            ]
        },
        "commands": [
            {
                "command": "vs-ctf.test",
                "category": "VS CTF",
                "title": "Test"
            }
        ]
    }
}
